#set($countNum =$!{startNum})
#foreach($c in $!{List_map})
<tr>
    <td>$countNum</td>
    <td>
        $c.name
    </td>
    <td>$!{c.note}</td>
    <td>$!{c.gmtCreate}</td>
    <td>$!{c.beginDate}</td>
    <td>$!{c.endDate}</td>
    <td>$!{c.status}</td>
    <td>
        #if(!$c.published)
            <a class="not-like-an-a course-handler" href="javascript:course_detail($c.id)" >修改</a>
            <a class="not-like-an-a course-handler" href="javascript:course_delete($c.id,$c.name)">删除</a>
        #else
            <a class="not-like-an-a course-handler" href="javascript:course_detail($c.id)">查看</a>
        #end
        #if($c.published && !$c.orderPlaced && !$c.paid)
            <a class="not-like-an-a course-handler" href="javascript:course_pull_back($c.id,$c.name)">撤回</a>
        #end
        <a class="not-like-an-a course-handler" href="javascript:course_copy($c.id)">复制新增</a>
        <a class="not-like-an-a course-handler" href="javascript:course_code($c.id)">二维码</a>
    </td>

</tr>

    #set($countNum = $countNum + 1)

#end